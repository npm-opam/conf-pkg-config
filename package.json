{
    "name": "conf-pkg-config-actual",
    "exportedEnvVars": {
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "1.0.0"
        },
        "conf_pkg_config_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "conf_pkg_config_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "1.0.0"
        },
        "conf_pkg_config_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        }
    },
    "version": "1.0.0",
    "dependencies": {
        "substs": "https://github.com/yunxing/substs.git",
        "nopam": "https://github.com/yunxing/nopam.git",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git",
        "yarn-pkg-config": "*"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && pkg-config --help && (opam-installer --prefix=$opam_prefix || true)"
    },
    "peerDependencies": {
        "@opam-alpha/ocaml": ">= 4.2.3"
    }
}
